{
	"tasks": [
	  {
		"type": "cppbuild",
		"label": "Build Philosophers",
		"command": "gcc",
		"args": [
		  "-o",
		  "${workspaceFolder}/build/Debug/philosophers",
		  "-g",
		  "philosophers.c",
		  "input.c"
		],
		"options": {
		  "cwd": "${workspaceFolder}"
		},
		"problemMatcher": [
		  "$gcc"
		],
		"group": {
		  "kind": "build",
		  "isDefault": true
		}
	  }
	],
	"version": "2.0.0"
  }
  